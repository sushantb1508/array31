def sequence(arr,n):
    d={}
    for i in arr:
        d[i]=True

    for i in range(1,n+1):
        if not i in d:
             return 0
        else:
            
            continue
    return 1
if __name__ == "__main__":
    arr=[1,1,3,2,4]
    arr=set(arr)
    n=len(arr)
    ans=sequence(arr,n)
    print(ans)
    
        
            
